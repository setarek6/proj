for(i=0;i<size;i++){
    for(j=0;j<size;j++){
        a=world2[i][j];
        for(b=-1;b<=1;b++){
            for(c=-1;c<=1;c++){
                if(world2[i+b][j+c]==a-1) {
                    world3[i+b][j+c]=a;
                }
            }
        }

    }
}



